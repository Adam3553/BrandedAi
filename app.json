{
    "name": "BRANDED-AI", 
    "logo": "https://te.legra.ph/file/e3c1ae96d329acd2d99cf.jpg",
    "description": "An Advanced Telegram UserId Chatbot.",
    "env": {
        "STRING": {
            "description": "Pyrogram String session of your account @BRANDEDSTRINGSESSION_BOT",
            "required": true
        },        
        "MONGO_URL": {
            "description": "mongo db url here",
